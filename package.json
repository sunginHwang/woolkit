{
  "name": "woolkit",
  "version": "0.0.1",
  "packageManager": "yarn@3.2.1",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    "types": "./dist/index.d.ts",
    "import": "./dist/index.esm.js",
    "require": "./dist/index.cjs.js"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:docs": "typedoc",
    "lint": "eslint .",
    "preview": "vite preview",
    "docs": "http-server -o ./docs/index.html",
    "docs:generate": "typedoc",
    "docs:start": "docusaurus start",
    "docs:build": "docusaurus build",
    "test": "vitest run",
    "release:package": "yarn build && changeset publish"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "dependencies": {
    "-": "^0.0.1",
    "@docusaurus/core": "3.7.0",
    "@docusaurus/preset-classic": "3.7.0",
    "@mdx-js/react": "^3.0.0",
    "clsx": "^2.0.0",
    "prism-react-renderer": "^2.3.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.28.1",
    "@docusaurus/module-type-aliases": "^3.7.0",
    "@docusaurus/tsconfig": "3.7.0",
    "@docusaurus/types": "^3.7.0",
    "@eslint/js": "^9.21.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.13.9",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react-swc": "^3.8.0",
    "docusaurus-plugin-typedoc": "^1.2.3",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "http-server": "^14.1.1",
    "jsdom": "^26.0.0",
    "path": "^0.12.7",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "typedoc": "^0.27.9",
    "typedoc-material-theme": "^1.3.0",
    "typedoc-plugin-markdown": "^4.4.2",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0",
    "vite-plugin-dts": "^4.5.3",
    "vitest": "^3.0.8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sunginHwang/woolkit"
  },
  "author": "bmo <gommpo111@gmail.com> (https://blog.woolta.com)",
  "contributors": [
    "bmo <gommpo111@gmail.com> (https://blog.woolta.com)"
  ]
}
